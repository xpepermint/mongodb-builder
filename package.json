{
  "name": "mongodb-builder",
  "version": "0.4.0",
  "description": "MongoDB migration framework.",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "ava": {
    "files": [
      "./dist/tests/*.test.js",
      "./dist/tests/**/*.test.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "example": "npm run build && node dist/example",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublish": "npm run build",
    "test": "npm run build && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/mongodb-builder.git"
  },
  "bugs": {
    "url": "https://github.com/xpepermint/mongodb-builder/issues"
  },
  "homepage": "https://github.com/xpepermint/mongodb-builder#readme",
  "keywords": [
    "mongo",
    "mongodb",
    "migration",
    "migrations",
    "patch",
    "patcher",
    "seed",
    "seeding",
    "patching",
    "migrating",
    "javascript",
    "nodejs",
    "typescript"
  ],
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "devDependencies": {
    "@types/globby": "^8.0.0",
    "@types/mongodb": "^3.1.17",
    "@types/node": "^10.12.12",
    "ava": "^0.25.0",
    "mongodb": "^3.1.10",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "globby": "^7.1.1"
  }
}
